module binaryExpressions
::this code module demonstrates binary expressions
{
  var number: float = 2.5;
  var flag: boolean;

  if number >= 2 && number * 2 <= 5 {
    flag = number + 1 < 3 || number - 0.1 > 2.3;
  }

  if flag && number % 0.5 == 0 {
    number = number ^ 2 ^ 3;
    number = number / 2;
  }

  flag = number != 0;
}
