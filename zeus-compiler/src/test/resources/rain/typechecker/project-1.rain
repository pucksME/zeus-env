[blueprint components]
blueprint component test {
  position {
    x = 10px;
    y = 10px;
    sorting = 0;
  }

  shape rectangle test1 {
    position {
       x = 0px;
       y = 0px;
       sorting = 0;
    }

    properties {
      background color = #000000;
      border color = #000000;
      shadow color = #000000;
      border radius = 0px, 0px, 0px, 0px;
      height = 10px;
      width = 10px;
      border width = 1px;
      shadow x = 1px;
      shadow y = 1px;
      shadow blur = 5px;
      background color enabled = true;
      border enabled = true;
      shadow enabled = true;
      visible = true;
      opacity = 100%;
    }
   }

